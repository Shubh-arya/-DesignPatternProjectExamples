package data_strucuture_and_algorithms.sort;

public class BubbleSort {

	public static void main(String[] args) {

	}

	static int[] bubbleSort(int[] arr) {
		for (int i : arr) {
			for (int j = 1; j < arr.length - 1; j++) {
				if (arr[j] < arr[j - 1]) {
					int temp = arr[j];
					arr[j] = arr[j - 1];
					arr[j - 1] = temp;
				}
			}
		}

		return arr;
	}
}
